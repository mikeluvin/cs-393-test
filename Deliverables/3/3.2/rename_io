#!/bin/bash
for i in {1..5}
do
    cp "input$i.json" "input$i-g.json"
    cp "output$i.json" "output$i-g.json"
    rm "input$i.json"
    rm "output$i.json"
done

for i in {6..10}
do
    cp "input$i.json" "input$i-b.json"
    cp "output$i.json" "output$i-b.json"
    rm "input$i.json"
    rm "output$i.json"
done